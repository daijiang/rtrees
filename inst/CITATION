citHeader("To cite rtrees in publications use:")

citEntry(
  entry    = "Article",
  title    = "rtrees: an R package to assemble phylogenetic trees from mega-trees",
  author   = "Daijiang Li",
  journal  = "v0.0.1",
  year     = "2022",
  url      = "https://github.com/daijiang/rtrees",
  textVersion = paste(
"Li Daijiang. rtrees: an R package to assemble phylogenetic trees from mega-trees. 2022. v0.0.1. https://github.com/daijiang/rtrees"
  )
)
